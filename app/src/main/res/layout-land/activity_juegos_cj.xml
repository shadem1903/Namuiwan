<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondojuegosed"
    tools:context=".Josue.JuegosCJ"
    tools:ignore="MissingDefaultResource">


    <Button
        android:id="@+id/btnPuzzle"
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:background="@drawable/puzzlejo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.969"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgjuegos"
        app:layout_constraintVertical_bias="0.106" />

    <Button
        android:id="@+id/btnHabilidades"
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:background="@drawable/habilidadesjo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.568"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgjuegos"
        app:layout_constraintVertical_bias="0.106" />

    <Button
        android:id="@+id/btnAdivina"
        android:layout_width="200dp"
        android:layout_height="80dp"
        android:background="@drawable/adivinajo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.568"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgjuegos"
        app:layout_constraintVertical_bias="0.557" />

    <Button
        android:id="@+id/btnMemorie"
        android:layout_width="200dp"
        android:layout_height="80dp"
        android:background="@drawable/memoriajo1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.962"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgjuegos"
        app:layout_constraintVertical_bias="0.557" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="150dp"
        android:layout_height="250dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.569"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgjuegos"
        app:layout_constraintVertical_bias="0.297"
        app:srcCompat="@drawable/palosenaled"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="150dp"
        android:layout_height="250dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.943"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgjuegos"
        app:layout_constraintVertical_bias="0.297"
        app:srcCompat="@drawable/palosenaled"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imgjuegos"
        android:layout_width="291dp"
        android:layout_height="98dp"
        android:layout_marginTop="16dp"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/imgjuegosed" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="7dp" />
</androidx.constraintlayout.widget.ConstraintLayout>