<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".yatulve.Habilidadesnew"
    android:background="@drawable/fondohabilidadesed">


    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="197dp"
        android:layout_height="270dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.565"
        app:layout_constraintStart_toStartOf="@+id/imghabilidades"
        app:layout_constraintTop_toTopOf="@+id/imghabilidades"
        app:layout_constraintVertical_bias="0.758"
        app:srcCompat="@drawable/palosenaled" />

    <Button
        android:id="@+id/imghabilidades"
        android:layout_width="247dp"
        android:layout_height="94dp"
        android:background="@drawable/titlehabilidades"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.053" />

    <ImageView
        android:id="@+id/imgFrutas"
        android:layout_width="236dp"
        android:layout_height="98dp"
        android:layout_marginStart="120dp"
        android:layout_marginTop="92dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.21"
        app:layout_constraintStart_toStartOf="@+id/imghabilidades"
        app:layout_constraintTop_toTopOf="@+id/imghabilidades"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/titlefrutas" />

    <ImageView
        android:id="@+id/imgtitlealimentosfrios"
        android:layout_width="236dp"
        android:layout_height="98dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imgFrutas"
        app:layout_constraintTop_toBottomOf="@+id/imghabilidades"
        app:layout_constraintVertical_bias="0.381"
        app:srcCompat="@drawable/titlealimentosfriosj" />

    <ImageView
        android:id="@+id/imgNumeros"
        android:layout_width="236dp"
        android:layout_height="98dp"
        android:layout_marginStart="148dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imgtitlealimentosfrios"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/imgtitlealimentosfrios"
        app:layout_constraintTop_toBottomOf="@+id/imghabilidades"
        app:layout_constraintVertical_bias="0.782"
        app:srcCompat="@drawable/titlenumeros" />


</androidx.constraintlayout.widget.ConstraintLayout>